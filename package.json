  },
  "main": "dist/kinetic.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/broadsw0rd/kinetic-engine"
  },
  "devDependencies": {
    "uglify-js": "latest",
    "jshint": "latest",
    "jshint-stylish": "latest",
    "babel": "5.8.29"
  },
  "babel": {
    "modules": "ignore",
    "blacklist": ["strict"],
    "loose": "all"
  },
  "jshintConfig":{
    "asi": true,
    "laxcomma": true,
    "laxbreak": true,
    "esnext": true,
    "proto": true
  },
  "scripts": {
    "build": "uglifyjs -c -m -- dist/kinetic.js > dist/kinetic.min.js",
    "lint": "jshint --reporter node_modules/jshint-stylish/index.js src/.",
    "6to5": "cat tools/header.js > dist/kinetic.js && babel src >> dist/kinetic.js && cat tools/footer.js >> dist/kinetic.js"
  },
  "dependencies": {}
}