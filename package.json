  "name": "kinetic-engine",
  "version": "0.0.1",
  "description": "Unprecedented kinetic engine",
  "keywords": [
    "kinetic",
    "drag`n`drop",
    "scroll",
    "swipe"
  ],
  "license": "MIT",
  "homepage": "",
  "bugs": {
